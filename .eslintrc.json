{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": 0,
        "no-underscore-dangle": 0,
        "no-unused-vars": 0,
        "max-len": 0,
        "no-shadow": 0,
        "no-useless-catch": 0
    }
}
